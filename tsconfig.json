{
  "files": [],
  "references": [{ "path": "./tsconfig.app.json" }, { "path": "./tsconfig.node.json" }],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],

      "@components": ["./src/components"],
      "@components/*": ["./src/components/*"],

      "@pages": ["./src/pages"],
      "@pages/*": ["./src/pages/*"],

      "@hooks": ["./src/hooks"],
      "@hooks/*": ["./src/hooks/*"],

      "@utils": ["./src/utils"],
      "@utils/*": ["./src/utils/*"],

      "@services": ["./src/services"],
      "@services/*": ["./src/services/*"],

      "@assets": ["./src/assets"],
      "@assets/*": ["./src/assets/*"]
    },
    "noImplicitAny": false,
    "noUnusedParameters": false,
    "skipLibCheck": true,
    "allowJs": true,
    "noUnusedLocals": false,
    "strictNullChecks": false
  }
}
